10 ' METEORO ESPACIAL
20 CLS 
30 Y=RND(-TIME)
40 SCREEN 1,1
50 DATA 0,0,8,8,54,127,28,8:A$=""
60 FOR I =1 TO 8
70   READ A
80   A$=A$+CHR$(A)
90 NEXT 
100 SPRITE$(1)=A$
110 WIDTH 32:KEY OFF:CLS:X=18:Z=0:Y=XS
120 H=INT(RND(1)*32)
130 IF RND(1)<.7 THEN LOCATE H,24:PRINT CHR$(249)
140 K=INT(RND(1)*32)
150 IF RND(1)<.6 THEN LOCATE K,24:PRINT CHR$(249)
160 PRINT:B=VPEEK(6528+X):C=VPEEK(6529+X)
170 IF B=249 OR C=249 THEN 230
180 PUT SPRITE 1,(8*X,88),7,1
190 D=STICK(0)
200 X=X-(D=3 AND X<30)+(D=7 AND X>0)
210 Z=Z+1
220 GOTO 140
230 LOCATE 2,18:PRINT "Seus pontos:";z:PLAY"fafa dede ecaca"